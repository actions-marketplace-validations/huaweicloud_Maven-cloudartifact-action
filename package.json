{
  "name": "maven-cloudartifact-action",
  "version": "1.0.0",
  "description": "Action for uploading or downloading artifact from HuaweiCloud CloudArtifact",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "run": "node ./lib/main.js",
    "package": "ncc build src/main.ts -o dist --license licenses.txt",
    "lint-create": "eslint --init",
    "lint": "eslint src --ext ts",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest",
    "all": "npm run lint && npm run format && npm run build && npm run package"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "prettier": "2.6.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@xmldom/xmldom": "^0.8.2",
    "xml-formatter": "^2.6.1"
  }
}
